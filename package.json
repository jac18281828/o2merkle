{
    "name": "o2merkle",
    "version": "0.0.1",
    "description": "Merkle Tree Optimisim Playground",
    "main": "index.js",
    "directories": {
        "doc": "docs",
        "lib": "lib",
        "contracts": "contracts",
        "test": "test"
    },
    "scripts": {
        "build": "forge build --sizes",
        "prettier:check": "prettier --check \"(contracts|test)/**.sol\"",
        "prettier:fix": "prettier --write \"(contracts|test)/**.sol\"",
        "lint": "solhint contracts/**/*.sol test/**/*.sol",
        "test": "~mr/.cargo/bin/forge test -vvv",
        "clean": "forge clean && forge cache clean"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jac18281828/o2merkle.git"
    },
    "devDependencies": {
        "solhint": "3.6.2"
    },
    "dependencies": {
        "prettier": "3.0.3",
        "prettier-plugin-solidity": "1.1.3"
    },
    "keywords": [],
